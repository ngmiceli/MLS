	<!DOCTYPE html>
	<html>
	  <head>
		<title><%= title %></title>
		<%= csrf_meta_tag %>
		<%= render 'layouts/general/stylesheets' %>
		<%= javascript_include_tag "application" %>
	  </head>
	  <body>
		<div class="container">
		  <%= render 'layouts/general/header' %>
		  <section class="round">
			<% flash.each do |key, value| %>
			  <div class="flash <%= key %>"><%= value %></div>
			<% end %>
			<%= yield %>
		  </section>
		  <%= render 'layouts/general/footer' %>
		  <%= debug(params) if Rails.env.development? %>
		</div>
	  </body>
	</html>